#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@connectrpc+protoc-gen-conn_5dbf2f0b7219f8169a1d6ffc61db35ee/node_modules/@connectrpc/protoc-gen-connect-es/bin/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@connectrpc+protoc-gen-conn_5dbf2f0b7219f8169a1d6ffc61db35ee/node_modules/@connectrpc/protoc-gen-connect-es/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@connectrpc+protoc-gen-conn_5dbf2f0b7219f8169a1d6ffc61db35ee/node_modules/@connectrpc/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@connectrpc+protoc-gen-conn_5dbf2f0b7219f8169a1d6ffc61db35ee/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@connectrpc+protoc-gen-conn_5dbf2f0b7219f8169a1d6ffc61db35ee/node_modules/@connectrpc/protoc-gen-connect-es/bin/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@connectrpc+protoc-gen-conn_5dbf2f0b7219f8169a1d6ffc61db35ee/node_modules/@connectrpc/protoc-gen-connect-es/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@connectrpc+protoc-gen-conn_5dbf2f0b7219f8169a1d6ffc61db35ee/node_modules/@connectrpc/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@connectrpc+protoc-gen-conn_5dbf2f0b7219f8169a1d6ffc61db35ee/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@connectrpc/protoc-gen-connect-es/bin/protoc-gen-connect-es" "$@"
else
  exec node  "$basedir/../@connectrpc/protoc-gen-connect-es/bin/protoc-gen-connect-es" "$@"
fi
