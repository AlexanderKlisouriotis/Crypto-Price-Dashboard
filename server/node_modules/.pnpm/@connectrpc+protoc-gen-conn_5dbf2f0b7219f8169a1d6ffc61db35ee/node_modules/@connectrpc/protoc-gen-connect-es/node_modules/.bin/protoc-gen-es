#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@bufbuild+protoc-gen-es@2.7.0_@bufbuild+protobuf@2.7.0/node_modules/@bufbuild/protoc-gen-es/bin/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@bufbuild+protoc-gen-es@2.7.0_@bufbuild+protobuf@2.7.0/node_modules/@bufbuild/protoc-gen-es/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@bufbuild+protoc-gen-es@2.7.0_@bufbuild+protobuf@2.7.0/node_modules/@bufbuild/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@bufbuild+protoc-gen-es@2.7.0_@bufbuild+protobuf@2.7.0/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@bufbuild+protoc-gen-es@2.7.0_@bufbuild+protobuf@2.7.0/node_modules/@bufbuild/protoc-gen-es/bin/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@bufbuild+protoc-gen-es@2.7.0_@bufbuild+protobuf@2.7.0/node_modules/@bufbuild/protoc-gen-es/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@bufbuild+protoc-gen-es@2.7.0_@bufbuild+protobuf@2.7.0/node_modules/@bufbuild/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/@bufbuild+protoc-gen-es@2.7.0_@bufbuild+protobuf@2.7.0/node_modules:/proc/cygdrive/c/Users/Alex/Documents/Pluto_Project/fullstack-coding-assessment-1--alexanderklisouriotis/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../@bufbuild/protoc-gen-es/bin/protoc-gen-es" "$@"
else
  exec node  "$basedir/../../../../@bufbuild/protoc-gen-es/bin/protoc-gen-es" "$@"
fi
